= form_for @result do |f|
  - if @result.errors.any?
    #error_explanation
      %h2= "#{pluralize(@result.errors.count, "error")} prohibited this result from being saved:"
      %ul
        - @result.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :combo
    = f.text_field :combo
  .field
    = f.label :reach
    = f.number_field :reach
  .field
    = f.label :frequency
    = f.number_field :frequency
  .field
    = f.label :total_respondents
    = f.number_field :total_respondents
  .field
    = f.label :total_respondents
    = f.number_field :total_respondents
  .actions
    = f.submit 'Save'
